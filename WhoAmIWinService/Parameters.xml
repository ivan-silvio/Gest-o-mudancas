<?xml version="1.0" encoding="utf-8" ?>
<parameters >

  <parameter name="My Setting" description="Please provide a custom value to setting 'MySetting'." defaultValue="ValueFromDefault" tags="">
    <parameterEntry kind="XmlFile" scope="\\*.exe.config$" match="/configuration/appSettings/add[@key='MySetting']/@value" />
  </parameter>

  <parameter name="MyDB-Web.config Connection String" description="MyDB SqlServer connString." defaultValue="ValueFromDefault" tags="">
    <parameterEntry kind="XmlFile" scope="\\*.exe.config$" match="/configuration/connectionStrings/add[@name='MyDB']/@value" />
  </parameter>
</parameters>